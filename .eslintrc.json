{
  "parser": "babel-eslint",
  "plugins": ["react", "jest", "cypress", "disable"],
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "plugin:cypress/recommended"
  ],
  "processor": "disable/disable",
  "env": {
    "browser": true,
    "es2020": true,
    "node": true,
    "cypress/globals": true
  },
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "spread": true,
    "templateStrings": true,
    "jsx": true
  },
  "rules": {
    "consistent-return": [0],
    "key-spacing": [0],
    "quotes": [0],
    "new-cap": [0],
    "no-multi-spaces": [0],
    "no-shadow": [0],
    "no-unused-vars": [1],
    "no-use-before-define": [2, "nofunc"],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  }
}
